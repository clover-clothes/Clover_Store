@model List<Product>
<h1>hello products</h1>
<table class="table-bordered table-striped">
    <thead>
    <tr>
        <th>
            name
        </th>
        <th>
            brand
        </th>
        <th>
            gender
        </th>

        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>
                    @obj.Title
                </td>
                <td>
                    @obj.Brand_name
                </td>
                <td>
                    @obj.Type
                </td>

            </tr>
            
        }
        

    </tbody>

</table>